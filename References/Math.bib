@book{Hestenes1987,
  address = {Norwell, {MA}},
  title = {Clifford algebra to geometric calculus},
  publisher = {Kluwer {Academic} {Publishers}},
  author = {Hestenes, {David} and {Sobczyk}, {Garret}},
  year = {1987},
  annote = {Note that, in this book, "The Geometric Algebra" refers to a sort of universal algebra of infinite dimension, and any particular geometric algebra is regarded as a sub-algebra of this larger one.  The particular algebras are distinguished by the choice of a pseudoscalar, which incorporates all vectors within that particular algebra.
Certainly, this is a sensible approach, but I don't think they made it clear.  And it certainly could be confusing to anyone who approaches the book having already seen GA in other contexts (where it is almost always defined more constructively).
In particular, I think this book would make a very poor introduction to the subject, though it certainly has many good points.}
}

